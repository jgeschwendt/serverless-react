{
  "name": "serverless-react",
  "version": "0.1.0",
  "description": "Serverless React Project Template",
  "main": "handler.ts",
  "scripts": {
    "build:server": "BABEL_ENV=serverless NODE_ENV=production sls webpack",
    "build:static": "BABEL_ENV=production NODE_ENV=production webpack --config config/webpack/webpack.config.static.js",
    "deploy:dev": "npm-run-all --serial build:static deploy:dev:server deploy:dev:static",
    "deploy:dev:server": "BABEL_ENV=serverless NODE_ENV=production sls deploy --stage dev",
    "deploy:dev:static": "sls client deploy --no-confirm --stage dev",
    "deploy:prod": "npm-run-all --serial build:static deploy:prod:server deploy:prod:static",
    "deploy:prod:server": "BABEL_ENV=serverless NODE_ENV=production sls deploy --stage prod",
    "deploy:prod:static": "sls client deploy --no-confirm --stage prod",
    "profile": "BABEL_ENV=production NODE_ENV=production webpack --config config/webpack/webpack.config.static.js --profile --json > dist/stats.json && webpack-bundle-analyzer dist/stats.json dist --mode static --no-open --report dist/report.html",
    "start:server": "BABEL_ENV=serverless NODE_ENV=development sls offline start --dontPrintOutput --host 0.0.0.0 --port 3001",
    "start:static": "BABEL_ENV=development NODE_ENV=development webpack-dev-server --config config/webpack/webpack.config.static.js",
    "test": "NODE_ENV=test jest",
    "tsc": "tsc --project tsconfig.json",
    "tslint": "tslint --project tsconfig.json",
    "tslint:fix": "tslint --fix --project tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jgeschwendt/serverless-react.git"
  },
  "keywords": [
    "aws",
    "serverless",
    "react"
  ],
  "author": "jlg",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/jgeschwendt/serverless-react/issues"
  },
  "homepage": "https://github.com/jgeschwendt/serverless-react#readme",
  "private": true,
  "resolutions": {
    "graphql": "0.13.2"
  },
  "dependencies": {
    "apollo-cache-inmemory": "1.2.2",
    "apollo-client": "2.3.2",
    "apollo-link-http": "1.5.4",
    "apollo-link-schema": "1.1.0",
    "graphql": "0.13.2",
    "graphql-tag": "2.9.2",
    "isomorphic-fetch": "2.2.1",
    "loadable-components": "2.2.2",
    "react": "16.4.0",
    "react-apollo": "2.1.4",
    "react-dom": "16.4.0",
    "react-ga": "2.5.3",
    "react-helmet": "5.2.0",
    "react-redux": "5.0.7",
    "react-router": "4.3.1",
    "react-router-config": "1.0.0-beta.4",
    "react-router-dom": "4.3.1",
    "redux": "4.0.0",
    "redux-form":"7.3.0",
    "sitemap": "1.13.0",
    "styled-components": "3.3.2"
  },
  "devDependencies": {
    "@babel/core": "7.0.0-beta.49",
    "@babel/preset-env": "7.0.0-beta.49",
    "@babel/preset-react": "7.0.0-beta.49",
    "@babel/preset-stage-0": "7.0.0-beta.49",
    "@babel/preset-typescript": "7.0.0-beta.49",
    "@types/react": "16.3.16",
    "@types/react-dom": "16.0.6",
    "aws-sdk": "2.252.1",
    "babel-loader": "8.0.0-beta.3",
    "babel-plugin-dynamic-import-node": "1.2.0",
    "babel-plugin-module-resolver": "3.1.1",
    "babel-plugin-styled-components": "1.5.1",
    "circular-dependency-plugin": "5.0.2",
    "jest": "23.1.0",
    "npm-run-all": "4.1.3",
    "serverless": "1.27.3",
    "serverless-finch": "2.1.2",
    "serverless-offline": "3.25.4",
    "serverless-webpack": "5.1.5",
    "tslint": "5.10.0",
    "tslint-config-standard": "7.0.0",
    "tslint-loader": "3.6.0",
    "tslint-react": "3.6.0",
    "typescript": "2.9.1",
    "webpack": "4.11.1",
    "webpack-bundle-analyzer": "2.13.1",
    "webpack-cli": "3.0.4",
    "webpack-dev-server": "3.1.4",
    "webpack-node-externals": "1.7.2"
  },
  "peerDependencies": {
    "aws-sdk": "*"
  }
}
